package apsII;
import javax.swing.JOptionPane;

public class aps {

	public static void main(String[] args) {
		int num, i, matricula[], idade[], qtdFuncionarios;
		String entrada;
		
		matricula = new int[2];
		idade = new int[2];
		
		i = 0;
		qtdFuncionarios = 0;
		
		while (qtdFuncionarios <= 2) {
			
			entrada = JOptionPane.showInputDialog("Digite sua matrÃ­cula");
			matricula[i] = Integer.parseInt(entrada);
			entrada = JOptionPane.showInputDialog("Digite sua idade");
			idade[i] = Integer.parseInt(entrada);
			
			
			qtdFuncionarios = qtdFuncionarios + 1;
		}
		
	}

}
